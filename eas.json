{
  "cli": {
    "version": ">= 13.2.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "projectDir": "apps/native"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_SERVER_URL": "https://my-better-t-app-web.vercel.app",
        "EXPO_PUBLIC_BYPASS_AUTH": "false"
      },
      "projectDir": "apps/native"
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_SERVER_URL": "https://my-better-t-app-web.vercel.app",
        "EXPO_PUBLIC_BYPASS_AUTH": "false"
      },
      "projectDir": "apps/native"
    }
  },
  "submit": {
    "production": {}
  }
}
